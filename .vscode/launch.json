{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Benchmark",
            "request": "launch",
            "mainClass": "com.badlogicgames.jnn.test.Benchmark",
            "projectName": "jnn"
        },
        {
            "type": "java",
            "name": "Benchmark",
            "request": "launch",
            "mainClass": "com.badlogicgames.jnn.test.Benchmark",
            "projectName": "gaan"
        },
        {
            "type": "java",
            "name": "Benchmark",
            "request": "launch",
            "mainClass": "com.badlogicgames.gann.test.Benchmark",
            "projectName": "gaan"
        },
        {
            "name": "server",
            "type": "node",
            "port": 9230,
            "request": "attach",
            "address": "localhost",
            "localRoot": "${workspaceFolder}/build",
            "remoteRoot": "/app/build",
            "restart": true,
            "continueOnAttach": true
        },
        {
            "name": "processor",
            "type": "node",
            "port": 9231,
            "request": "attach",
            "address": "localhost",
            "localRoot": "${workspaceFolder}/build",
            "remoteRoot": "/app/build",
            "restart": true,
            "continueOnAttach": true
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "client",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/html"
        }
    ],
    "compounds": [
        {
            "name": "dev",
            "configurations": ["server", "processor", "client"]
        }
    ]
}
